{
  "name": "kiln-it",
  "version": "1.0.0",
  "description": "\"Keep records of clay percent shrinkage, weight and dates of firing for my clay creations\"",
  "main": "index.js",
  "engines": {
    "node": "16.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilyparkes/kiln-it.git"
  },
  "author": "Emily Coco Parkes",
  "keywords": [
    "fullstack",
    "web",
    "development",
    "practice"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emilyparkes/kiln-it/issues"
  },
  "homepage": "https://github.com/emilyparkes/kiln-it#readme",
  "scripts": {
    "start": "node server",
    "dev": "npm run dev:client & npm run dev:server",
    "dev:client": "webpack --config client/webpack.config.js --watch",
    "dev:server": "nodemon server",
    "build": "npm run client && npm run db:migrate",
    "client": "webpack --config client/webpack.config.js",
    "server": "node server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db": "npm run db:migrate && npm run db:seed",
    "db:seed": "npm run knex seed:run",
    "db:migrate": "npm run knex migrate:latest",
    "db:rollback": "npm run knex migrate:rollback",
    "db:reset": "npm run knex migrate:rollback && npm run db",
    "lint": "eslint --ext .js,.jsx client server",
    "check": "npm run lint && npm run test",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "test:client": "jest /client --watchAll --verbose",
    "test:server": "jest /server --watchAll --verbose"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "cloudinary": "^1.32.0",
    "cloudinary-core": "^2.13.0",
    "express": "^4.17.2",
    "knex": "^2.1.0",
    "pg": "^8.7.1",
    "sqlite3": "^5.0.8",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.13.14",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@mui/styles": "^5.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^12.7.1",
    "authenticare": "^0.4.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "http-errors": "^1.8.0",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.3.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "sass": "^1.50.1",
    "sass-loader": "12.6.0",
    "style-loader": "^3.3.0",
    "supertest": "^6.2.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.3"
  }
}
